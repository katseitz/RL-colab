        <Param val="h_t = random.randint(1, 2)&amp;#10;if h_t == 1:&amp;#10;    good_side = &quot;right&quot;&amp;#10;else:&amp;#10;    good_side = &quot;left&quot;&amp;#10;&amp;#10;num_rewarded = 0&amp;#10;num_switch = 0&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
